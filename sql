--table created by create command--
create table student(
  s_name varchar(100),
  s_id int,
  Age int,
  class varchar(100)
  );
--insert a values by insert command--
insert into student values
("gobi",222511,20,"final year"),
("guhan",222512,19,"final year"),
("haridoss",222513,21,"final year"),
("hariprasant",222514,20,"final year"),
("harish",222511,20,"final year");
--retrive the student table by select command--
select * from student;
